let g:solarized_termcolors=256
" set t_Co=16
set background=dark
colorscheme solarized

" move temp files to tmp (stored in ram, avoid ssd writes)
set backupdir=/private/tmp
set dir=/private/tmp

map <leader>p :set paste!<CR>
nmap <silent> <leader>s :set nolist!<CR>

set tabstop=4
set shiftwidth=4
set softtabstop=4

