[[ "$BASHRC_ALREADY_PROCESSED" == "$$" ]] && return
BASHRC_ALREADY_PROCESSED=$$

[ -f /etc/bash_completion ] && . /etc/bash_completion
for F in $(find -L ~/.dotfiles -name \*.bash); do source $F; done

set -o vi
export EDITOR=vim
[ -d "$HOME/bin" ] && export PATH="$HOME/bin:$PATH"
export HISTFILE="$HOME/.history/bash"

